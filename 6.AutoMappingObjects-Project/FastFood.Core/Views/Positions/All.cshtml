@model IList<FastFood.Core.ViewModels.Positions.PositionsAllViewModel>

@{
    ViewData["Title"] = "All Positions";
}
<h1 class="text-center">All PositionsCategories</h1>
<hr class="hr-2" />
<table class="table mx-auto">
    <thead>
        <tr class="row">
            <th class="col-md-1">#</th>
            <th class="col-md-2">Position</th>
        </tr>
    </thead>
    <tbody>
        @for(var i = 1; i < Model.Count(); i++)
    {
        <tr class="row">
            <th class="col-md-1">@i</th>
            <td class="col-md-2">@Model[i].Name</td>
        </tr>
    }
    </tbody>
</table>